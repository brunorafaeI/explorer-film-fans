{
  "name": "films-fans-project",
  "version": "1.0.0",
  "description": "A API Rocketseat made by Explorer program",
  "main": "build/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch -r tsconfig-paths/register src/server.ts",
    "migrate": "npx knex migrate:latest --knexfile ./build/infra/database/knex/index.js",
    "ci": "npm run migrate && npm start",
    "start": "npm build && node build/server.js"
  },
  "author": "Josiel Nascimento",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "knex": "^2.5.1",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.5",
    "@types/pg": "^8.10.2",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "nodemon": "^3.0.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  }
}
